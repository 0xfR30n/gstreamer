; -*- scheme -*-

(define-method flags
  (of-object "GstElement")
  (c-name "GST_FLAGS")
  (return-type "GstElementFlags")
)

(define-method set_flag
  (of-object "GstObject")
  (c-name "GST_FLAG_SET")
  (return-type "none")
  (parameters
   '("GstObjectFlags" "flags")
   )
)

(define-method unset_flag
  (of-object "GstObject")
  (c-name "GST_FLAG_UNSET")
  (return-type "none")
  (parameters
   '("GstObjectFlags" "flag")
   )
)